{
  "name": "ircbloq-link-desktop",
  "productName": "Ircbloq Link",
  "version": "1.0.0",
  "description": "IrcBloq link desktop",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && npm run cpdevice && npm run cpext && npm run cptools",
    "cpdevice": "cpy ./ ../../../devices --parents --cwd=./node_modules/ircbloq-device/src",
    "cpext": "cpy ./ ../../../extensions --parents --cwd=./node_modules/ircbloq-extension/src",
    "cptools": "cpy ./ ../../../tools --parents --cwd=./node_modules/ircbloq-link/tools",
    "start": "electron .",
    "build": "npm run build:pack",
    "build:pack": "electron-builder --dir",
    "build:dist": "npm run cpdevice && npm run cpext && npm run cptools && electron-builder",
    "clean": "rimraf dist devices extensions tools",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ircbloqcc/ircbloq-link-desktop.git"
  },
  "keywords": [
    "ircbloq",
    "link",
    "scratch3.0",
    "desktop"
  ],
  "author": "IrcBloq Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ircbloqcc/ircbloq-link-desktop/issues"
  },
  "homepage": "https://github.com/ircbloqcc/ircbloq-link-desktop#readme",
  "dependencies": {
    "compare-versions": "^3.6.0",
    "del": "^6.0.0",
    "ircbloq-device": "0.1.0-prerelease.20210328115506",
    "ircbloq-extension": "0.1.0-prerelease.20210329032033",
    "ircbloq-link": "0.1.0-prerelease.20210326122102"
  },
  "devDependencies": {
    "cpy-cli": "^3.1.1",
    "electron": "^8.5.5",
    "electron-builder": "^22.9.1",
    "rimraf": "^3.0.2"
  }
}
